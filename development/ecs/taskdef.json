{
    "executionRoleArn": "arn:aws:iam::456247443832:role/cicd-practice-EcsTaskExecuteRole",
    "containerDefinitions": [
        {
            "name": "development-task",
            "image": "<IMAGE1_NAME>",
            "memory": 512,
            "cpu": 256,
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 80,
                    "protocol": "tcp",
                    "containerPort": 80
                }
            ],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/cicd-practice-cluster/cicd-practice-development-service",
                    "awslogs-region": "us-west-1",
                    "awslogs-stream-prefix": "development"
                }
            },
            "environment": [
                {
                    "name": "appname",
                    "value": "development"
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "family": "development-task"
}
